{
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "printWidth": 120,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "importOrder": [
    "^vitest",
    "^es-toolkit",
    "<THIRD_PARTY_MODULES>",
    "^@(.*)$",
    "^[.]/",
    "^[.]{2,}/"
  ],
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}
